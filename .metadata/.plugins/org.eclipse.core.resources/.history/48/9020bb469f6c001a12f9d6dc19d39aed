package application;

import javafx.beans.property.SimpleStringProperty;
import javafx.scene.control.TableColumn;


public class Table{
		private final SimpleStringProperty number;
		private final SimpleStringProperty title;
		private final SimpleStringProperty state;
		
		public Table(SimpleStringProperty inWon, SimpleStringProperty firstColum,SimpleStringProperty state) {
			this.number=inWon;
			this.title=firstColum;
			this.state=state;
			
			
		}

		public TableColumn<Table, String> getNumber() {
			return number;
		}

		public TableColumn<Table, String> getTitle() {
			return title;
		}

		public TableColumn<Table, String> getState() {
			return state;
		}
		
}