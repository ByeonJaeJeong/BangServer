package application;

import javafx.beans.property.SimpleStringProperty;
import javafx.scene.control.TableColumn;


public class Table{
		private final TableColumn<Table, String> number;
		private final TableColumn<Table, String> title;
		private final TableColumn<Table, String> state;
		
		public Table(TableColumn<Table, String> inWon, TableColumn<Table, String> firstColum, TableColumn<Table, String> state) {
			this.number=new SimpleStringProperty(inWon);
			this.title=new SimpleStringProperty(firstColum);
			this.state=new SimpleStringProperty(state);
			
			
		}

		public SimpleStringProperty getNumber() {
			return number;
		}

		public SimpleStringProperty getTitle() {
			return title;
		}

		public SimpleStringProperty getState() {
			return state;
		}
		
}